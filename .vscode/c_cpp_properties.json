{
  "configurations": [
    {
      "name": "clang-build",
      "compilerPath": "/usr/bin/clang-16",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/src",
        "/usr/include"
      ],
      "cStandard": "gnu23",
      "cppStandard": "c++20",
      "configurationProvider": "ms-vscode.cmake-tools",
      "compileCommands": "${workspaceFolder}/build/x64-linux-clang-static/compile_commands.json",
      "browse": {
        "path": [
          "${workspaceFolder}",
          "/usr/include"
        ],
        "limitSymbolsToIncludedHeaders": false
      }
    }
  ],
  "version": 4,
  "enableConfigurationSquiggles": true
}
